{
  "name": "youkol-clone",
  "version": "1.0.0",
  "description": "A proxy server for handling image enhancement API requests and user authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "pocketbase": "cd pocketbase && (pocketbase serve || pocketbase.exe serve)",
    "dev": "concurrently \"npm run server\" \"npm run pocketbase\"",
    "setup": "node install.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "canvas": "3.1.0",
    "cookie-parser": "1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "1.18.1",
    "form-data": "4.0.2",
    "multer": "^1.4.5-lts.1",
    "pocketbase": "0.25.2",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
